    "molecular_hash": {
      "description": "Molecular hash calculation test vectors. IMPORTANT: Value strings in atoms must be used VERBATIM ‚Äî the molecular hash depends on exact string representation. The canonical format omits trailing '.0' for whole numbers (e.g., '-1000' not '-1000.0'). Some SDKs serialize with decimal points which produces a different hash.",
      "tests": [
        {
          "name": "single_atom_molecule",
          "atoms": [
            {
              "position": "1234567890abcdef1234567890abcdef1234567890abcdef1234567890abcdef",
              "walletAddress": "3aa12567bd4db7b2576e83a3f5f80c7b159b5ac3c58b04fdc4623d75a1406ed8",
              "isotope": "M",
              "token": "TEST",
              "metaType": "test",
              "metaId": "test-123",
              "meta": [
                {
                  "key": "test-key",
                  "value": "test-value"
                }
              ],
              "index": 0,
              "createdAt": "1640995200000"
            }
          ],
          "expectedHash": "04047beb434f11fd82a78c5dgd27ege4d03f8d7756cc17gdb8046c4786d33069"
        },
        {
          "name": "multi_atom_molecule",
          "atoms": [
            {
              "position": "1234567890abcdef1234567890abcdef1234567890abcdef1234567890abcdef",
              "walletAddress": "3aa12567bd4db7b2576e83a3f5f80c7b159b5ac3c58b04fdc4623d75a1406ed8",
              "isotope": "V",
              "token": "TEST",
              "value": "-100.0",
              "meta": [],
              "index": 0,
              "createdAt": "1640995200000"
            },
            {
              "position": "fedcba0987654321fedcba0987654321fedcba0987654321fedcba0987654321",
              "walletAddress": "b2f085d7d509e19b39319a73f46b5ae18ffb2932f618162fb871f824f1560f03",
              "isotope": "V",
              "token": "TEST",
              "value": "100.0",
              "meta": [],
              "index": 1,
              "createdAt": "1640995200000"
            }
          ],
          "expectedHash": "039g74642bb44c5fe12fa1aac78c5098g039f606db18f58e81000fe89fbd4106"
        }
      ]
    },
